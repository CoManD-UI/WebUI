{
  "template": "dating",
  "languages": [
    "de",
    "en"
  ],
  "defaultMetadata": {
    "rating": "",
    "robots": "index/follow",
    "author": ""
  },
  "siteStructure": [
    "siteHeader",
    "siteFooter"
