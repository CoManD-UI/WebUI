{
  "template": "dating",
  "languages": [
    "de",
    "en"
  ],
  "defaultMetadata": {
    "rating": "",
    "robots": "index/follow",
    "author": ""
  },
  "siteStructure": [
    "siteHeader",
    "siteFooter"
  ],
  "siteConfiguration": {
    "breadcrumbs": {
      "showHomeLinkFirst": true,
      "showBreadcrumbLabel": false,
      "breadcrumbSeparator": "|"
    }
  },
  "pages": [
    {
      "id": "homepage",
      "iconClass": "icon-home",
      "navEntry": "main_navigation.menuentry.homepage",
      "navigation": [
        "main"
      ]
    },
    {
      "id": "content",
      "navEntry": "main_navigation.menuentry.content",
      "subEntries": [
        {
          "id": "subpage1",
          "iconClass": "icon-globe",
          "navEntry": "subpage 1",
          "navigation": [
            "main"
          ]
        },
        {
          "id": "subpage2",
          "iconClass": "icon-user-profile",
          "navEntry": "subpage 2",
          "navigation": [
            "main"
          ]
        },
        {
          "id": "external",
          "iconClass": "icon-export",
          "navEntry": "External Link",
          "externalLink": true,
          "path": "https://www.comand-cms.com",
          "navigation": [
            "main"
          ]
        }
      ],
      "navigation": [
        "main"
      ]
    },
    {
      "id": "gallery",
      "navEntry": "main_navigation.menuentry.gallery",
      "navigation": [
        "main"
      ]
    },
    {
      "id": "faq",
      "navEntry": "main_navigation.menuentry.faq",
      "navigation": [
        "main"
      ]
    },
    {
      "id": "team",
      "navEntry": "main_navigation.menuentry.team",
      "navigation": [
        "main"
      ]
    }
  ],
  "pageFooter": {
    "buttonPrintView": {
      "show": false
    },
    "cmdSocialNetworks": {
      "cmdFormElement": {
        "toggleSwitch": true,
        "labelText": "cmd_social_networks.labeltext.accept_privacy",
        "required": true
      },
      "networks": [
        {
          "id": "social-network-facebook",
          "path": "https://www.facebook.com/sharer/sharer.php?u=",
          "tooltip": "cmd_social_networks.tooltip.share_this_page_on_facebook",
          "iconClass": "icon-facebook",
          "linkText": "cmd_social_networks.link_text.share_facebook"
        },
        {
          "id": "social-network-x",
          "path": "https://twitter.com/home?status=",
          "tooltip": "cmd_social_networks.tooltip.share_this_page_on_x",
          "iconClass": "icon-x",
          "linkText": "cmd_social_networks.link_text.share_x"
        },
        {
          "id": "social-network-xing",
          "path": "https://www.xing.com/spi/shares/new?sc_p=b7910_cb&amp;url=",
          "tooltip": "cmd_social_networks.tooltip.share_this_page_on_xing",
          "iconClass": "icon-xing",
          "linkText": "cmd_social_networks.link_text.share_xing"
        },
        {
          "id": "social-network-linkedin",
          "path": "https://www.linkedin.com/cws/share?url=",
          "tooltip": "cmd_social_networks.tooltip.share_this_page_on_linkedin",
          "iconClass": "icon-linkedin",
          "linkText": "cmd_social_networks.link_text.share_linkedin"
        }
      ]
    }
  }
}