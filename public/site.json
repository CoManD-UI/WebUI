  "siteConfiguration": {
    "breadcrumbs": {
      "showBreadcrumbs": true,
      "showHomeLinkFirst": true,
      "showBreadcrumbLabel": true,
      "breadcrumbSeparator": "|"
    },
    "cookieDisclaimer": {
      "useCookieDisclaimer": true,
      "buttonLabelAcceptAllCookies": "Accept all cookies",
      "buttonLabelAcceptCurrentSettings": "Accept current settings",
      "privacyText": "By browsing this website you accept the usage and saving of anonymous data!",
      "required": {
        "cmdHeadline": {
          "headlineText": "Required cookies"
        },
        "cookies": [
          {
            "id": "cookie-sessionid",
            "value": "sessionid",
            "description": "This cookie is required for functionality",
            "labelText": "SessionID",
            "disabled": true,
            "linkDataPrivacy": {
              "label": "Details for this cookie:",
              "link": "/content/data-privacy-en.html",
              "linkText": "Open data privacy"
            }
          },
          {
            "id": "cookie-google-recaptcha",
            "value": "google-recaptcha",
            "description": "cookie_disclaimer.google_recaptcha.description",
            "labelText": "cookie_disclaimer.google_recaptcha.labeltext",
            "disabled": true,
            "linkDataPrivacy": {
              "label": "cookie_disclaimer.google_recaptcha.link_data_privacy.label",
              "link": "cookie_disclaimer.google_recaptcha.link_data_privacy.link",
              "target": "_blank",
              "linkText": "cookie_disclaimer.google_recaptcha.link_data_privacy.link_text"
            }
          }
        ]
      },
      "optional": {
        "cmdHeadline": {
          "headlineText": "Optional cookies"
        },
        "cookies": [
          {
            "id": "cookie-google-maps",
            "value": "google-maps",
            "description": "cookie_disclaimer.google_maps.description",
            "labelText": "cookie_disclaimer.google_maps.labeltext",
            "disabled": false,
            "linkDataPrivacy": {
              "label": "cookie_disclaimer.google_maps.link_data_privacy_label.label",
              "link": "cookie_disclaimer.google_maps.link_data_privacy_label.link",
              "target": "_blank",
              "linkText": "cookie_disclaimer.google_maps.link_data_privacy_label.link_text"
            }
          },
          {
            "id": "cookie-google-analytics",
            "value": "google-analytics",
            "description": "This cookie collects anonymous data for statistics and improvements",
            "labelText": "Google Analyticsâ„¢",
            "linkDataPrivacy": {
              "label": "Details for this cookie:",
              "link": "https://policies.google.com/privacy?hl=en-US",
              "target": "_blank",
              "linkText": "Open data privacy from Google"
            }
          }
        ]
      }
    }
  },
